        "pinia": "^2.3.1",
        "pinia-plugin-persistedstate": "^4.2.0",
        "sass": "^1.83.4",
        "vue": "^3.5.13",
        "vue-echarts": "^7.0.3",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^19.6.1",
