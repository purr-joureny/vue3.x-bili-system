{
    "name": "vue3.x-bili-system",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "pnpm run -r build && vite",
        "build": "pnpm run -r build && vue-tsc -b && vite build",
